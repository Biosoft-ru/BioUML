<?xml version="1.0" encoding="UTF-8"?> 
<dynamicPropertySet> 
    <property name="Proxy" type="com.developmentontheedge.beans.Preferences" display-name="Proxy" short-description="Proxy preferences" ordered="true">
        <dynamicPropertySet> 
            <property name="Use" type="java.lang.Boolean" value="false" display-name="Use proxy" short-description="Use proxy"/> 
            <property name="Host" type="java.lang.String" value="amber.developmentontheedge.com" display-name="Host" short-description="Proxy host"/> 
            <property name="Port" type="java.lang.Integer" value="8080" display-name="Port" short-description="Proxy port"/> 
            <property name="Username" type="java.lang.String" value="" display-name="Username" short-description="Proxy username"/> 
            <property name="Password" type="java.lang.String" display-name="Password" short-description="Proxy password"/> 
        </dynamicPropertySet> 
    </property> 
	<property name="Global" type="com.developmentontheedge.beans.Preferences" display-name="Global" short-description="Global" hidden="true">
        <dynamicPropertySet> 
            <property name="NotationPath" type="java.lang.String" value="databases/Utils/Diagrams/graphic notations" display-name="NotationPath" short-description="NotationPath"/> 
            <property name="ApplicationName" type="java.lang.String" value="BioUML server" display-name="ApplicationName" short-description="Application name"/> 
            <property name="ServerLink" type="java.lang.String" value="http://server.biouml.org/bioumlweb" display-name="ServerLink" short-description="URL to server"/> 
            <property name="SmtpHost" type="java.lang.String" value="amber.developmentontheedge.com" display-name="SmtpHost" short-description="Smtp host"/> 
            <property name="SmtpFrom" type="java.lang.String" value="support@biouml.org" display-name="SmtpFrom" short-description="Mail from value"/> 
            <property name="AdminMail" type="java.lang.String" value="ivan@dote.ru" display-name="AdminMail" short-description="Mail of server admin"/> 
            <property name="PublicFolderServerPath" type="java.lang.String" value="/opt/tomcat/webapps/examples" display-name="PublicFolderServerPath" short-description="Path to public folder"/> 
            <property name="PublicFolderURL" type="java.lang.String" value="http://localhost:8080/examples/" display-name="PublicFolderURL" short-description="HTML address of public folder"/> 
            <property name="UpgradeAccountURL" type="java.lang.String" value="http://server.biouml.org/bioumlweb" display-name="UpgradeAccountURL" short-description="Link to upgrade account form"/> 
			<property name="JabberServer" type="java.lang.String" value="http://biostore.biouml.org:7070/http-bind/" display-name="Jabber server URL" short-description="Link to the Jabber server web frontend"/> 
        </dynamicPropertySet> 
    </property> 
    <property name="Launchers" type="com.developmentontheedge.beans.Preferences" display-name="Process launchers">
        <dynamicPropertySet>
            <property name="LocalLauncher" type="com.developmentontheedge.beans.Preferences">
                <dynamicPropertySet>
                     <property name="class" type="java.lang.String" value="ru.biosoft.tasks.process.LocalProcessLauncher"/>
                     <!--property name="sharedFolder" type="java.lang.String" value="/tmp/galaxy"/-->
                </dynamicPropertySet>
            </property>
        </dynamicPropertySet>
    </property>
    <property name="NodeConfig" type="com.developmentontheedge.beans.Preferences" display-name="Node configuration">
        <dynamicPropertySet>
            <property name="Path" type="java.lang.String" value="C:\projects\java\bioumlNode"/> 
            <property name="Launcher" type="java.lang.String" value="LocalLauncher"/> 
            <property name="JavaPath" type="java.lang.String" value="java"/> 
            <property name="JavaOpts" type="java.lang.String" value="-Xms256m -Xmx1024m"/> 
            <!--property name="JavaOpts" type="java.lang.String" value="-Xms256m -Xmx1024m -Xdebug -Xnoagent -Djava.compiler=NONE -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=5006"/--> 
            <property name="ServerLink" type="java.lang.String" value="http://localhost:8080/biouml"/> 
        </dynamicPropertySet>
    </property>
    <property name="JAVA_LIBRARY_PATH" type="java.lang.String" value="" display-name="java.library.path" short-description="Extra library path for plugins"/>
</dynamicPropertySet> 

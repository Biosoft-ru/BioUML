	<extension id="Quadratic Hill-climbing" point="biouml.plugins.optimization.method">
		<analysisClass class="ru.biosoft.analysis.optimization.methods.GoldfeldOptMethod" name="Quadratic Hill-climbing" description="ru/biosoft/analysis/optimization/resources/goldfeld.html" />

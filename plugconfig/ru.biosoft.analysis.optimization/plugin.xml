<?xml version="1.0" encoding="UTF-8"?>
<plugin>

	<extension id="Evolution strategy (SRES)" point="biouml.plugins.optimization.method">
		<analysisClass class="ru.biosoft.analysis.optimization.methods.SRESOptMethod" name="Evolution strategy (SRES)" description="ru/biosoft/analysis/optimization/resources/sres.html" />
	</extension>

	<extension id="GLBSOLVE" point="biouml.plugins.optimization.method">
		<analysisClass class="ru.biosoft.analysis.optimization.methods.GlbSolveOptMethod" name="GLBSOLVE" description="ru/biosoft/analysis/optimization/resources/glbsolve.html" />

                                <exclude>${biouml.plugin.jar.excludes.extra17}</exclude>
                                <exclude>${biouml.plugin.jar.excludes.extra18}</exclude>
                                <exclude>${biouml.plugin.jar.excludes.extra19}</exclude>
                                <exclude>${biouml.plugin.jar.excludes.extra20}</exclude>

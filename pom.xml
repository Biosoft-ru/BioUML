       <pluginManagement>
        <plugins>
            <!-- Plugin to manipulate properties -->
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>build-helper-maven-plugin</artifactId>
                <version>3.4.0</version>
                <executions>
                    <execution>
                        <id>replace-dots-with-slashes</id>
                        <goals>
                            <goal>regex-property</goal>
                        </goals>
                        <configuration>
                            <name>biouml.plugin.path</name>
                            <value>${project.artifactId}</value>
                            <regex>\.</regex> <!-- Matches the dot character -->
                            <replacement>/</replacement> <!-- Replaces dot with slash -->
                        </configuration>
                    </execution>
                </executions>
            </plugin>

            <!-- Jar plugin to package the plugin into a jar -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <version>3.4.0</version>
                <executions>
                    <execution>
                        <!--id>package-plugin</id-->
                        <id>default-jar</id>
                        <phase>package</phase>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                        <configuration>                            
                            <classesDirectory>${rootDir}/src/target/classes</classesDirectory>
                            <outputDirectory>${rootDir}/target/plugins</outputDirectory>
                            <finalName>${project.artifactId}_${project.version}</finalName>
                            <includes>
                                <include>${biouml.plugin.path}/**</include>
                            </includes>
                            <excludes>
                                <exclude>**/_test/</exclude>
                                <exclude>${biouml.plugin.jar.excludes}</exclude>
                                <exclude>${biouml.plugin.jar.excludes.extra1}</exclude>
                                <exclude>${biouml.plugin.jar.excludes.extra2}</exclude>
                                <exclude>${biouml.plugin.jar.excludes.extra3}</exclude>

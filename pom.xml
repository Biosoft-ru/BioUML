                                <exclude>${biouml.plugin.jar.excludes.extra29}</exclude>
                                <exclude>${biouml.plugin.jar.excludes.extra30}</exclude>
                            </excludes>
                            <archive> 
                               <manifestFile>${pluginDir}/META-INF/MANIFEST.MF</manifestFile>
                               <manifestEntries>

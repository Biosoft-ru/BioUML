                                           <exclude name="${biouml.plugin.jar.excludes.extra29}" />
                                           <exclude name="${biouml.plugin.jar.excludes.extra30}" />
                                       </patternset>
                                   </fileset>
                                   <zipgroupfileset dir="${pluginDir}" includes="**/*.jar" excludes="src.jar" />
                                </jar>
                            </target>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>

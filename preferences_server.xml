<?xml version="1.0" encoding="UTF-8"?> 
<dynamicPropertySet> 
	<property name="Global" type="com.developmentontheedge.beans.Preferences" display-name="Global" short-description="Global" hidden="true">
        <dynamicPropertySet> 
            <property name="NotationPath" type="java.lang.String" value="databases/Utils/Diagrams/graphic notations" display-name="NotationPath" short-description="NotationPath"/> 
            <property name="ApplicationName" type="java.lang.String" value="BioUML server" display-name="ApplicationName" short-description="Application name"/> 
            <property name="ServerLink" type="java.lang.String" value="http://localhost:8080/bioumlweb" display-name="ServerLink" short-description="URL to server"/> 
            <property name="SmtpHost" type="java.lang.String" value="mail" display-name="SmtpHost" short-description="Smtp host"/> 
            <property name="SmtpFrom" type="java.lang.String" value="support@biouml.org" display-name="SmtpFrom" short-description="Mail from value"/> 
            <property name="AdminMail" type="java.lang.String" value="support@biouml.org" display-name="AdminMail" short-description="Mail of server admin"/> 
        </dynamicPropertySet> 
    </property> 
    <property name="Launchers" type="com.developmentontheedge.beans.Preferences" display-name="Process launchers">
        <dynamicPropertySet>
            <property name="LocalLauncher" type="com.developmentontheedge.beans.Preferences">
                <dynamicPropertySet>
                     <property name="class" type="java.lang.String" value="ru.biosoft.tasks.process.LocalProcessLauncher"/>
                     <!--property name="sharedFolder" type="java.lang.String" value="/tmp/galaxy"/-->
                </dynamicPropertySet>
            </property>
        </dynamicPropertySet>
    </property>
    <property name="WebLogConfig" type="com.developmentontheedge.beans.Preferences" display-name="Web log config">
        <dynamicPropertySet>
                <property name="useWebLog" type="java.lang.Boolean" value="true"/>
                <property name="Level" type="java.lang.String" value="WARNING"/>
                <property name="LogLimit" type="java.lang.Integer" value="120"/>
        </dynamicPropertySet>
    </property>
    <property name="JAVA_LIBRARY_PATH" type="java.lang.String" value="" display-name="java.library.path" short-description="Extra library path for plugins"/>
</dynamicPropertySet> 

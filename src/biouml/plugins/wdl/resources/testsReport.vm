<html>
<head>
<title></title>
</head>
<body>
<h2>WDL</h2>
<table class="details" cellspacing="2" cellpadding="5" width="95%" border="1">
<tbody>
<tr valign="top" style="border: medium solid ;">
<th align="center" valign="middle">#</th>
<th align="center" valign="middle">Name</th>
<th align="center" valign="middle">Description</th>
<th align="center" valign="middle">Diagram generated</th>
<th align="center" valign="middle">WDL generated</th>
<th align="center" valign="middle">Round test</th>
<th align="center" valign="middle">WDL validated</th>
<th align="center" valign="middle">Nextflow generated</th>
<th align="center" valign="middle">Nextflow executed</th>
</tr>
#set ($index = 1)
#foreach($result in $results)
#set($name = $result.getName())
<tr>
<td>$index</td>
<td><a href="${name}/${name}.html" target="_blank">$result.getTitle()</a></td>
<td>$result.getDescription()</a></td>
<td><a href="${name}/${name}.png" target="_blank">$result.getDiagramGenerated()</a></td>
<td><a href="${name}/${name}_exported.wdl" target="_blank">$result.getWDLGenerated()</a></td>
<td><a href="${name}/${name}_round.wdl" target="_blank">$result.getWDLGenerated()</a></td>
<td>$result.getWDLValidated()</td>
<td><a href="${name}/${name}.nf" target="_blank">$result.getNextflowGenerated()</a></td>
<td>$result.getNextflowExecuted()</td>
</tr>
#set ($index = $index + 1)
#end
</tbody>
</body>
</html>

/*
 * Convert diagram action
 */
{
    id: "convert",
    label: "Преобразование к типу",
    icon: "icons/convert.gif",
    visible: "function(doc)
    {
    	if ((doc != null) && (doc instanceof Diagram)) 
        {    
            return true;
        }
        return -1; 
    }",
    action: "function(event)
    {
        var doc = getActiveDocument();
        if(doc.convert)
        {
        	doc.convert();
        }
    }"
}
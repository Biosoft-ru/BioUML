/*
 * Logout action
 */
{
    id: "logout",
    label: "Выход из системы",
    icon: "icons/logout.gif",
    visible: "function(doc)
    {
        if(isAnonymousSession)
    	   return -1;
	   return true;
    	
    }",
    action: "function(event)
    {
        var user = new User();
        user.logout(false);    
    }"
}